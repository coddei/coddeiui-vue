{
    "presets": [
        ["@babel/env", {"loose": true, "modules": false}],
        "@babel/typescript",
        "babel-preset-typescript-vue"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators",
            { "legacy": true }
        ],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }]
    ]
}